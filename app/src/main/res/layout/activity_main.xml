<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="dialog" />

    <Button
        android:id="@+id/music"
        android:layout_toRightOf="@id/dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="soundPool" />

    <Button
        android:id="@+id/music2"
        android:layout_toEndOf="@id/music"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="mediaPlayer" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/all"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/dialog" />

    <com.zhou.test.view.NewFeedView
        android:id="@+id/new_feed_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:button_marginBottom="64dp" />

</RelativeLayout>
